  <Clock name="var_inh_allAttr_2"    declaredType="td_allAttr"    valueReference="7" intervalVariability="triggered" resolution="2"
         canBeDeactivated="false" priority="2" intervalDecimal="2.0"
         shiftDecimal="2.0" supportsFraction="false" intervalCounter="2"
         shiftCounter="2"/>
