<ModelExchange modelIdentifier="myModelIdentifier" />

<TypeDefinitions>
  <!-- NOTE:
    It doesn't make much sense to put intervalVariability on typedefs when they must be
    overriden on variables, but the schema files require it.
  -->
  <Clock name="td_minimal"    intervalVariability="constant"/>
  <Clock name="td_resolution" intervalVariability="constant" resolution="99"/>
  <Clock name="td_allAttr"    intervalVariability="countdown"
         canBeDeactivated="true" priority="99" intervalDecimal="99.0"
         shiftDecimal="99.0" supportsFraction="true" resolution="99"
         intervalCounter="99" shiftCounter="99"/>
</TypeDefinitions>

<ModelVariables>
  <!-- Tests inheritance from typedef -->
  <Clock name="var_default_type_1"                                valueReference="0" intervalVariability="constant"/>
  <Clock name="var_inh_minimal_1"    declaredType="td_minimal"    valueReference="1" intervalVariability="constant"/>
  <Clock name="var_inh_resolution_1" declaredType="td_resolution" valueReference="2" intervalVariability="constant"/>
  <Clock name="var_inh_allAttr_1"    declaredType="td_allAttr"    valueReference="3" intervalVariability="triggered"/>

  <!-- Tests that attributes on variables get precedence, and that other attributes are still inherited -->
  <Clock name="var_default_type_2"                                valueReference="4" intervalVariability="constant"/>
  <Clock name="var_inh_minimal_2"    declaredType="td_minimal"    valueReference="5" intervalVariability="constant"/>
  <Clock name="var_inh_resolution_2" declaredType="td_resolution" valueReference="6" intervalVariability="constant"
         resolution="1"/>
  <Clock name="var_inh_allAttr_2"    declaredType="td_allAttr"    valueReference="7" intervalVariability="triggered"
         canBeDeactivated="false" priority="2" intervalDecimal="2.0"
         shiftDecimal="2.0" supportsFraction="false" resolution="2"
         intervalCounter="2" shiftCounter="2"/>
</ModelVariables>

<ModelStructure/>

